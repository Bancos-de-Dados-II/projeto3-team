generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Instituicao {
  id           String   @id @default(auto()) @map("_id") @db.ObjectId
  name         String
  cnpj         String   @unique
  contact      String
  description  String
  localization Json
  createdAt    DateTime @default(now())
  updatedAt    DateTime @default(now()) @updatedAt
}